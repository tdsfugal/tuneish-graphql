{
  "name": "web",
  "version": "0.0.8-pre",
  "description": "NextJS variant of Tuneish web app",
  "author": "Tim Smith <tim.smith@fugaltech.com",
  "license": "LicenseRef-LICENSE",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -r .next",
    "clobber": "nvm use && yarn install",
    "c-build-dev": "docker build -f Dockerfile.dev -t web-next-dev:latest .",
    "c-build": "docker build -t web-next:latest .",
    "c-dev": "docker run -it -d --rm -p 3000:3000 --name web-next-dev web-next-dev:latest",
    "c-start": "docker run -it -d --rm -p 3000:3000 --name web-next web-next:latest",
    "c-stop": "docker stop web-next",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test-silent": "jest --watch --silent"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "framer-motion": "10.12.6",
    "graphql": "^16.7.1",
    "next": "^13.4.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^6.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.26",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "prettier": "^3.0.0",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "packageManager": "yarn@3.6.1"
}
